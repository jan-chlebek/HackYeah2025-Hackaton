# Backend
ASPNETCORE_ENVIRONMENT=Production
ConnectionStrings__DefaultConnection=Server=<AZURE_PG_HOST>;Database=uknf_db;User Id=uknf_user;Password=<PG_PASSWORD>;Ssl Mode=Require;Trust Server Certificate=true
